    "nameOfSchool": "Zonal Secretary Office, Sagamu"
  },
  {
    "nameOfSchool": "Zonal Secretary Office, Yewa North"
  },
  {
    "nameOfSchool": "Zonal Secretary Office, Yewa South"
  }]
  function matchAndConvertToCSV(typedSchools, upToDateSchools) {
  
    // Extract location from school name
    function extractLocation(name) {
      if (!name || typeof name !== 'string') return '';
      
      // Common location patterns: after comma, in parentheses at end, after school type
      const patterns = [
        /,\s*([^,]+)$/,           // After last comma
        /\(([^)]+)\)$/,           // In parentheses at end
        /(?:High|Grammar|Secondary|College|School)\s+(.+?)(?:,|$)/i
      ];
      
      for (const pattern of patterns) {
        const match = name.match(pattern);
        if (match && match[1]) {
          return match[1].toLowerCase().trim();
        }
      }
      
      return '';
    }
    
    // Normalize name for comparison - extract actual name parts
    function extractNameParts(name) {
      if (!name || typeof name !== 'string') return [];
      
      // Remove level indicators and parentheses first
      let cleanName = name
        .replace(/\(snr\.?\)|senior/gi, '')
        .replace(/\(jnr\.?\)|junior/gi, '')
        .replace(/\(comb\.?\)|combined/gi, '')
        .replace(/\[.*?\]/g, '');  // Remove square brackets
      
      return cleanName
        .toLowerCase()
        .replace(/mrs?\.|miss|dr\.|prof\./gi, '') // remove titles
        .replace(/[.,]/g, '')                      // remove punctuation
        .replace(/\s+/g, ' ')                      // normalize spaces
        .trim()
        .split(' ')
        .filter(word => word.length > 2);          // only words with 3+ chars
    }
  
    // Detect school level (Senior, Junior, Combined)
    function detectSchoolLevel(schoolName) {
      const lowerName = schoolName.toLowerCase();
      
      // Check for Senior indicators
      if (/\b(snr\.?|senior)\b/i.test(lowerName)) {
        return 'SENIOR';
      }
      
      // Check for Junior indicators
      if (/\b(jnr\.?|junior)\b/i.test(lowerName)) {
        return 'JUNIOR';
      }
      
      // Check for Combined indicators
      if (/\b(comb\.?|combined)\b/i.test(lowerName)) {
        return 'COMBINED';
      }
      
      return 'UNKNOWN';
    }
  
    // Check if location matches
    function locationMatch(loc1, loc2) {
      if (!loc1 || !loc2) return 0.5; // Neutral if location missing
      
      const l1 = loc1.toLowerCase().replace(/[.,\s]/g, '');
      const l2 = loc2.toLowerCase().replace(/[.,\s]/g, '');
      
      // Exact match
      if (l1 === l2) return 1.5;
      
      // One contains the other
      if (l1.includes(l2) || l2.includes(l1)) return 1.2;
      
      // Check for partial matches (at least 3 chars)
      if (l1.length >= 3 && l2.length >= 3) {
        if (l1.includes(l2.substring(0, 3)) || l2.includes(l1.substring(0, 3))) {
          return 0.9;
        }
      }
      
      // No match - penalty
      return 0.4;
    }
  
    // Better name matching with location awareness
    function calculateNameMatch(typedSchool, dbSchool) {
      // Get school levels
      const typedLevel = detectSchoolLevel(typedSchool);
      const dbLevel = detectSchoolLevel(dbSchool);
      
      // Extract locations
      const typedLocation = extractLocation(typedSchool);
      const dbLocation = extractLocation(dbSchool);
      
      const typedParts = extractNameParts(typedSchool);
      const dbParts = extractNameParts(dbSchool);
      
      if (typedParts.length === 0 || dbParts.length === 0) return 0;
      
      // Filter out very generic words that cause false matches
      const genericWords = ['community', 'high', 'school', 'grammar', 'secondary', 
                            'comprehensive', 'college', 'academy'];
      
      const significantTypedParts = typedParts.filter(p => !genericWords.includes(p));
      const significantDbParts = dbParts.filter(p => !genericWords.includes(p));
      
      // If no significant parts remain, fall back to all parts
      const partsToMatch = significantTypedParts.length > 0 ? significantTypedParts : typedParts;
      const partsToCompare = significantDbParts.length > 0 ? significantDbParts : dbParts;
      
      // Count matches
      let exactMatchCount = 0;
      let partialMatchCount = 0;
      
      partsToMatch.forEach(typedPart => {
        // Exact match
        if (partsToCompare.includes(typedPart)) {
          exactMatchCount++;
        } else {
          // Partial match (one word contains the other, min 4 chars)
          partsToCompare.forEach(dbPart => {
            if (typedPart.length >= 4 && dbPart.length >= 4) {
              if (typedPart.includes(dbPart) || dbPart.includes(typedPart)) {
                partialMatchCount += 0.5;
              }
            }
          });
        }
      });
      
      // Calculate base score - require higher percentage of matches
      const totalMatches = exactMatchCount + partialMatchCount;
      
      // Need at least 50% of significant parts to match
      if (totalMatches < partsToMatch.length * 0.5) {
        return 0;
      }
      
      let score = totalMatches / Math.max(partsToMatch.length, 2);
      
      // Apply location multiplier
      const locationMultiplier = locationMatch(typedLocation, dbLocation);
      score *= locationMultiplier;
      
      // Apply level matching bonus/penalty
      if (typedLevel === dbLevel && typedLevel !== 'UNKNOWN') {
        // Exact level match - significant bonus
        score *= 1.5;
      } else if (typedLevel === 'COMBINED' && dbLevel === 'COMBINED') {
        // Combined matches combined - bonus
        score *= 1.4;
      } else if (typedLevel === 'SENIOR' && dbLevel === 'JUNIOR') {
        // Senior typed but Junior in DB - major penalty
        score *= 0.2;
      } else if (typedLevel === 'JUNIOR' && dbLevel === 'SENIOR') {
        // Junior typed but Senior in DB - major penalty
        score *= 0.2;
      } else if (typedLevel === 'COMBINED' && (dbLevel === 'SENIOR' || dbLevel === 'JUNIOR')) {
        // Combined can match either Senior or Junior - acceptable
        score *= 0.85;
      } else if ((typedLevel === 'SENIOR' || typedLevel === 'JUNIOR') && dbLevel === 'COMBINED') {
        // Specific level typed but Combined in DB - acceptable match
        score *= 0.85;
      } else if (typedLevel !== 'UNKNOWN' && dbLevel !== 'UNKNOWN' && typedLevel !== dbLevel) {
        // Different levels - major penalty
        score *= 0.3;
      }
      
      // Penalty for matching too many generic schools
      const genericScore = exactMatchCount / (exactMatchCount + partialMatchCount + 1);
      if (genericScore < 0.5) {
        score *= 0.7; // Reduce score if mostly partial matches
      }
      
      // Cap score at 1.0
      return Math.min(score, 1.0);
    }
  
    // Assign confidence level based on score
    function getConfidenceLevel(score) {
      if (score >= 0.8) return 'High Confidence';
      if (score >= 0.5) return 'Medium Confidence';
      if (score >= 0.3) return 'Low Confidence';
      return 'Very Low Confidence';
    }
  
    // Find best match for each typed school
    const matchedResults = [];
    const usedIndices = new Set(); // Track used matches to avoid duplicates
    
    typedSchools.forEach((typedSchool, index) => {
      let bestMatch = null;
      let bestScore = 0;
      let bestIndex = -1;
      
      upToDateSchools.forEach((dbSchool, dbIndex) => {
        // Skip if already matched (uncomment to prevent duplicate matches)
        // if (usedIndices.has(dbIndex)) return;
        
        const score = calculateNameMatch(typedSchool, dbSchool.nameOfSchool || '');
        
        if (score > bestScore) {
          bestScore = score;
          bestMatch = dbSchool;
          bestIndex = dbIndex;
        }
      });
      
      // Only match if score is reasonable (at least 40% match)
      if (bestMatch && bestScore >= 0.4) {
        usedIndices.add(bestIndex);
        matchedResults.push({
          serialNo: index + 1,
          typedSchool: typedSchool.replace(/,$/, '').trim(),
          matchedSchool: bestMatch.nameOfSchool,
          matchScore: bestScore.toFixed(2),
          status: getConfidenceLevel(bestScore)
        });
      } else {
        // No good match found
        matchedResults.push({
          serialNo: index + 1,
          typedSchool: typedSchool.replace(/,$/, '').trim(),
          matchedSchool: 'NO MATCH FOUND',
          matchScore: '0.00',
          status: 'No Match'
        });
      }
    });
    
    return matchedResults;
  }
  
  // Convert to CSV
  function arrayToCSV(data) {
    if (!data || data.length === 0) return '';
    
    const headers = Object.keys(data[0]);
    
    function escapeCSV(value) {
      if (value == null) return '';
      const stringValue = String(value);
      if (stringValue.includes(',') || stringValue.includes('"') || stringValue.includes('\n')) {
        return `"${stringValue.replace(/"/g, '""')}"`;
      }
      return stringValue;
    }
    
    let csv = headers.map(escapeCSV).join(',') + '\n';
    
    data.forEach(row => {
      const values = headers.map(header => escapeCSV(row[header]));
      csv += values.join(',') + '\n';
    });
    
    return csv;
  }
  
  // MAIN FUNCTION
  function matchSchoolsAndExportCSV(typedSchools, upToDateSchools) {
    const matchedData = matchAndConvertToCSV(typedSchools, upToDateSchools);
    const csvContent = arrayToCSV(matchedData);
    
    // Calculate statistics
    const stats = {
      total: matchedData.length,
      highConfidence: matchedData.filter(m => m.status === 'High Confidence').length,
      mediumConfidence: matchedData.filter(m => m.status === 'Medium Confidence').length,
      lowConfidence: matchedData.filter(m => m.status === 'Low Confidence').length,
      veryLowConfidence: matchedData.filter(m => m.status === 'Very Low Confidence').length,
      noMatch: matchedData.filter(m => m.status === 'No Match').length
    };
    
    console.log('=== MATCHING STATISTICS ===');
    console.log(`Total: ${stats.total}`);
    console.log(`High Confidence: ${stats.highConfidence} (${(stats.highConfidence/stats.total*100).toFixed(1)}%)`);
    console.log(`Medium Confidence: ${stats.mediumConfidence} (${(stats.mediumConfidence/stats.total*100).toFixed(1)}%)`);
    console.log(`Low Confidence: ${stats.lowConfidence} (${(stats.lowConfidence/stats.total*100).toFixed(1)}%)`);
    console.log(`Very Low Confidence: ${stats.veryLowConfidence} (${(stats.veryLowConfidence/stats.total*100).toFixed(1)}%)`);
    console.log(`No Match: ${stats.noMatch} (${(stats.noMatch/stats.total*100).toFixed(1)}%)`);
    
    return {
      matchedData,
      csvContent,
      statistics: stats,
      csvBlob: new Blob([csvContent], { type: 'text/csv;charset=utf-8;' })
    };
  }
  

  console.log('=== TESTING IMPROVED MATCHER ===\n');
  const result = matchSchoolsAndExportCSV(typedProposedSchool, dbSchoolsJson);
  
  console.log('\n=== SAMPLE RESULTS ===');
  result.matchedData.slice(0, 10).forEach(item => {
    console.log(`${item.serialNo}. "${item.typedSchool}"  "${item.matchedSchool}" (${item.matchScore}, ${item.status})`);
  });
  
  console.log('\n=== CSV PREVIEW  ===');
  const csvLines = result.csvContent
  console.log(csvLines);
